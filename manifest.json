{
  "name": "DarkMojira",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Dark Theme for Mojira",
  "author": "FX - PR0CESS",
  "homepage_url": "https://github.com/fxmorin/DarkMojira",
  "icons" : {
    "16": "icons/mojira16.png",
    "32": "icons/mojira32.png",
    "48": "icons/mojira48.png",
    "64": "icons/mojira64.png",
    "128": "icons/mojira128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://bugs.mojang.com/*", "file://*/*"],
      "css": ["src/FxDarkTheme.css"],
      "js": ["src/mojiraLogo.js"]
    }
  ]
}
